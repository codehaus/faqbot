<project>
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.codehaus.faqbot</groupId>
  <artifactId>faqbot</artifactId>
  <packaging>pom</packaging>
  <version>0.1-SNAPSHOT</version>
  <ciManagement>
    <system>continuum</system>
    <notifiers>
      <notifier>
        <configuration>
          <address>dev@faqbot.codehaus.org</address>
        </configuration>
      </notifier>
    </notifiers>
  </ciManagement>
  <developers>
    <developer>
      <id>jietang</id>
      <name>Jie Tang</name>
      <email>jietang86@gmail.com</email>
      <organization>Codehaus</organization>
      <roles>
        <role>Designer</role>
        <role>Developer</role>
        <role>Project Manager</role>
      </roles>
    </developer>
    <developer>
      <id>vmassol</id>
      <name>Vincent Massol</name>
      <email>vmassol@codehaus.org</email>
      <organization>Codehaus</organization>
      <roles>
        <role>Mentor</role>
      </roles>
      <timezone>+1</timezone>
    </developer>
  </developers>
  <organization>
    <name>Codehaus</name>
    <url>http://codehaus.org/</url>
  </organization>
  <build>
    <sourceDirectory>C:\eclipse-config\workspace\faqbot\src\main\java</sourceDirectory>
    <scriptSourceDirectory>src/main/scripts</scriptSourceDirectory>
    <testSourceDirectory>C:\eclipse-config\workspace\faqbot\src\test\java</testSourceDirectory>
    <directory>C:\eclipse-config\workspace\faqbot\target</directory>
    <outputDirectory>C:\eclipse-config\workspace\faqbot\target\classes</outputDirectory>
    <testOutputDirectory>C:\eclipse-config\workspace\faqbot\target\test-classes</testOutputDirectory>
    <resources>
      <resource>
        <directory>C:\eclipse-config\workspace\faqbot\src\main\resources</directory>
      </resource>
    </resources>
    <testResources>
      <testResource>
        <directory>C:\eclipse-config\workspace\faqbot\src\test\resources</directory>
      </testResource>
    </testResources>
    <finalName>faqbot-0.1-SNAPSHOT</finalName>
    <plugins>
      <plugin>
        <artifactId>maven-jar-plugin</artifactId>
        <version>2.0-beta-1-SNAPSHOT</version>
      </plugin>
      <plugin>
        <artifactId>maven-jar-plugin</artifactId>
        <version>2.0-beta-1-SNAPSHOT</version>
      </plugin>
      <plugin>
        <artifactId>maven-jar-plugin</artifactId>
        <version>2.0-beta-1-SNAPSHOT</version>
      </plugin>
    </plugins>
  </build>
  <modules>
    <module>tequilabot</module>
  </modules>
  <repositories>
    <repository>
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
      <id>central</id>
      <name>Maven Repository Switchboard</name>
      <url>http://repo1.maven.org/maven2</url>
    </repository>
  </repositories>
  <pluginRepositories>
    <pluginRepository>
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
      <id>central-plugins</id>
      <name>Maven Plugin Repository</name>
      <url>http://repo1.maven.org/maven2/plugins</url>
    </pluginRepository>
  </pluginRepositories>
</project>