	xsi:schemaLocation="http://aitools.org/programd/4.5/test-cases http://aitools.org/programd/4.5/test-cases.xsd"
	name="suite1" clearInput="CLEAR VARS">
	<TestCase>
		<Input>CONNECT</Input>
		<ExpectedAnswer>Hello! My name is ROBOT. Won't you tell me your name?</ExpectedAnswer>
	</TestCase>
	<TestCase>
		<Input>MY NAME IS ALBERT</Input>
		<ExpectedKeywords>Nice to meet</ExpectedKeywords>
		<ExpectedKeywords>pleasure</ExpectedKeywords>
		<ExpectedKeywords>Glad to know</ExpectedKeywords>
		<ExpectedKeywords>disposal</ExpectedKeywords>
	</TestCase>
	<TestCase>
		<Input>tell me more</Input>
		<ExpectedAnswer>Ahh yes yes, ALBERT, i know you... whatever...</ExpectedAnswer>
	</TestCase>
</TestSuite>
